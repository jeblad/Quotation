{
	"name": "Citation",
	"version": "0.1.0",
	"author": [
		"John Erling Blad"
	],
	"url": "https://github.com/jeblad/citation",
	"namemsg": "citation-extensionname",
	"descriptionmsg": "citation-desc",
	"license-name": "GPL v2",
	"type": "parserhook",
	"AutoloadClasses": {
		"Citation\\Decorator":
			"includes/Citation/Decorator.php",
		"Citation\\Hooks":
			"includes/Citation/Hooks.php",
		"Citation\\Quote":
			"includes/Citation/Quote.php",

		"Citation\\Parser\\HtmlParser":
			"includes/Citation/Parser/HtmlParser.php",
		"Citation\\Parser\\IParser":
			"includes/Citation/Parser/IParser.php",

		"Citation\\Job\\Validation":
			"includes/Citation/Job/Validation.php",
		"Citation\\Job\\ValidationJob":
			"includes/Citation/Job/ValidationJob.php",

		"Citation\\Test\\HtmlParserTest":
			"tests/phpunit/includes/Citation/HtmlParserTest.php",

		"Citation\\Test\\QuoteTest":
			"tests/phpunit/includes/Citation/QuoteTest.php"
	},
	"config": {
		"_prefix": "wgCitation",
		"DelayedValidation": false
	},
	"callback": "Citation\\Hooks::onExtensionSetup",
	"ExtensionMessagesFiles": {
	},
	"Hooks": {
		"UnitTestsList": [
			"Citation\\Hooks::onUnitTestsList"
		],
		"ParserFirstCallInit": [
			"Citation\\Hooks::onParserFirstCallInit"
		]
	},
	"MessagesDirs": {
		"Citation": [
			"i18n"
		]
	},
	"ResourceModules": {
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Citation/modules"
	},
	"TrackingCategories": [
	],
	"SpecialPages": {
	},
	"LogTypes": [
	],
	"JobClasses": {
		"Validation": "Citation\\Job\\ValidationJob"
	},
	"manifest_version": 1
}